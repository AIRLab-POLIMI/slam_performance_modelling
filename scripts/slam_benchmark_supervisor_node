#!/usr/bin/env python

import slam_benchmark_supervisor_ros.slam_benchmark_supervisor
import rospy

if __name__ == '__main__':

    rospy.init_node('slam_benchmark_supervisor', anonymous=False)

    node = slam_benchmark_supervisor_ros.slam_benchmark_supervisor.SlamBenchmarkSupervisor()
    try:
        node.loop()
    except rospy.exceptions.ROSInterruptException:
        pass
